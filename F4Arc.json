{
    "title": "F4 to Arc Browser",
    "rules": [
        {
            "description": "Open Arc app with F4",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f4",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a Arc.app"
                        }
                    ]
                }
            ]
        }
    ]
}
